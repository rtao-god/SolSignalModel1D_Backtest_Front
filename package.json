{
    "name": "hackathon-expo",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "main": "src/app/main.tsx",
    "scripts": {
        "dev": "vite",
        "build": "tsc -b && vite build",
        "lint": "npm run lint:eslint && npm run lint:prettier",
        "lint:eslint": "eslint \"src/**/*.{ts,tsx}\" --fix",
        "lint:prettier": "prettier --write \"src/**/*.{ts,tsx,scss}\"",
        "preview": "vite preview",
        "prepare": "husky"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run precommit && lint-staged && git add ."
        }
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx}": [
            "eslint --fix",
            "prettier --write"
        ],
        "*.{css,scss,sass}": [
            "prettier --write"
        ]
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/rtao-god/Advanced_Frontend.git"
    },
    "homepage": "https://github.com/rtao-god/Advanced_Frontend#readme",
    "bugs": "https://github.com/rtao-god/Advanced_Frontend/issues",
    "author": "Rtao-god",
    "keywords": [],
    "license": "ISC",
    "peerDependencies": {
        "eslint": "^8.56.0"
    },
    "dependencies": {
        "@reduxjs/toolkit": "^2.2.7",
        "@tanstack/react-query": "^5.90.12",
        "axios": "^1.9.0",
        "husky": "^9.1.7",
        "i18next": "^23.12.2",
        "i18next-browser-languagedetector": "^8.0.0",
        "i18next-http-backend": "^2.6.2",
        "jspdf": "^4.1.0",
        "jspdf-autotable": "^5.0.2",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-i18next": "^15.0.0",
        "react-redux": "^9.1.2",
        "react-router-dom": "^6.25.1",
        "recharts": "^3.5.1",
        "zustand": "^5.0.8"
    },
    "devDependencies": {
        "@tanstack/react-query-devtools": "^5.91.1",
        "@testing-library/dom": "^10.4.1",
        "@testing-library/react": "^16.3.0",
        "@types/node": "^22.0.0",
        "@types/react": "^18.3.3",
        "@types/react-dom": "^18.3.0",
        "@typescript-eslint/eslint-plugin": "^7.15.0",
        "@typescript-eslint/parser": "^7.15.0",
        "@vitejs/plugin-react": "^4.7.0",
        "commitlint": "^19.5.0",
        "csstype": "^3.2.3",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-react": "^7.37.5",
        "eslint-plugin-react-hooks": "^4.6.2",
        "eslint-plugin-react-refresh": "^0.4.7",
        "framer-motion": "^11.3.19",
        "globals": "^13.24.0",
        "miragejs": "^0.2.0-alpha.3",
        "prettier": "^3.3.3",
        "react-query": "^3.39.3",
        "sass": "^1.77.8",
        "typescript": "^5.2.2",
        "typescript-eslint": "^7.18.0",
        "vite": "^6.4.1",
        "vite-plugin-svgr": "^4.5.0"
    },
    "optionalDependencies": {
        "@rollup/rollup-linux-x64-gnu": "^4.19.1"
    }
}
